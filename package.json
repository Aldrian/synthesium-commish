{
  "name": "synthesium-commishs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "draft-convert": "^2.1.2",
    "draft-js": "^0.10.5",
    "graphql-request": "^1.6.0",
    "medium-draft": "^0.5.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "reactcss": "^1.2.3",
    "redux": "^4.0.0",
    "redux-localstorage-simple": "^2.1.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-preset-react-app": "^3.1.2",
    "eslint": "^4.19.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "flow-bin": "^0.47.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "prettier": "^1.10.2",
    "prettier-eslint-cli": "4.7.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "extract-messages-osx": "NODE_ENV=production babel ./src  --out-file /dev/null",
    "lint:files": "prettier-eslint ./src/*.js ./src/**/*.js ./src/*.jsx ./src/**/*.jsx .babelrc .eslintrc",
    "lint": "npm run lint:files -- --list-different",
    "format": "npm run lint -- --write",
    "precommit": "lint-staged",
    "extract-messages": "set NODE_ENV=production&& babel ./src >NUL"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "useTabs": true,
    "bracketSpacing": false
  },
  "lint-staged": {
    "stories/**": [
      "prettier-eslint --write",
      "git add"
    ],
    "app/scripts/**.{js,jsx}": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
